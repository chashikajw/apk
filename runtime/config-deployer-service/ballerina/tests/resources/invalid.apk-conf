id: "123e4567-e89b-12d3-a456-426614174000"
name: "PetStore API"
basePath: "/petstore"
version: "1.0.0"
type: "REST"
operations:
  - target: "/pets"
    verb: "GET"
    secured: true
    endpointConfigurations:
      sandbox:
        endpoint: "https://petstore.swagger.io/v2/pet"
  - target: "/pets/{petId}"
    verb: "GET"
    secured: true
    scopes: []
    endpointConfigurations:
      production:
        endpoint: "https://petstore.swagger.io/v2/pet"
  - target: "/cat/{petId}"
    verb: "GET"
    secured: true
    scopes: []      
authentication:
  - authType: "OAuth2"
    sendTokenToUpstream: false
    enabled: true
    headerName: "Authorization"
    queryParamName: "apikey"
    headerEnable: true
    queryParamEnable: true
